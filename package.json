{
  "name": "festivalpos-backen",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "type": "commonjs",
  "devDependencies": {
    "@flydotio/dockerfile": "^0.5.7",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/memory-cache": "^0.2.5",
    "@types/node": "^16.11.10",
    "nodemon": "^3.1.1",
    "prettier": "3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "4.5.2"
  },
  "dependencies": {
    "-": "^0.0.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "memory-cache": "^0.2.0",
    "pg": "^8.4.0",
    "pg-connection-string": "^2.6.4",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.3.20"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon - exec 'ts-node' src/index.ts",
    "test": "npm run test:types && npm run test:format",
    "test:types": "tsc --noEmit",
    "test:format": "npx prettier . --check",
    "format": "npx prettier . --write",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:generate": "npm run typeorm migration:generate -- -d src/data-source.ts",
    "typeorm:migrate": "npm run typeorm migration:run -- -d ./src/data-source.ts",
    "typeorm:revert": "npm run typeorm migration:revert -- -d src/data-source.ts",
    "typeorm:drop": "npm run typeorm schema:drop -- -d src/data-source.ts",
    "migrate-and-start": "npm run typeorm:migrate && npm run start"
  }
}
